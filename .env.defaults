# Tracked, non-secret defaults for local start and deployments.
# Values in `.env` and `.env.local` (gitignored) override these defaults.

# Images
# Image prefix for published images (forks should set this to their own GHCR namespace)
WEGENT_IMAGE_PREFIX=ghcr.io/papalqi
WEGENT_BACKEND_VERSION=1.35.4
WEGENT_FRONTEND_VERSION=1.35.4
WEGENT_EXECUTOR_MANAGER_VERSION=1.35.4
# Codex-enabled executor tag
WEGENT_EXECUTOR_VERSION=1.35.4-codex

# Ports (optional)
WEGENT_FRONTEND_PORT=3000
WEGENT_BACKEND_PORT=8000
WEGENT_EXECUTOR_MANAGER_PORT=8001

# Feature flags
CODEX_SHELL_ENABLED=true
# Code Shell resume semantics (Codex: resume_session_id, ClaudeCode: session_id reuse)
CODE_SHELL_RESUME_ENABLED=true
WEGENT_EXECUTOR_IMAGE=ghcr.io/papalqi/wegent-executor:1.35.4-codex
WEGENT_EXECUTOR_MANAGER_IMAGE=ghcr.io/papalqi/wegent-executor-manager:1.35.4
